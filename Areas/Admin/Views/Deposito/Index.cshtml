
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <div class="col-lg-6">
        <h2 class="text-primary">Lista de Depositos</h2>
    </div>
    <div class="col-lg-6">
        <a class="btn btn-outline-primary" asp-action="Upsert"><i class="bi bi-plus-square-fill"></i>Crear Deposito</a>
    </div>
</div>

<br />

<div class="p-4 border rounded bg-light">
    <table id="tblDatos" class="table table-responsive table-hover">
      <thead class="table-dark">
          <tr>
              
              <th>Nombre</th>
              <th>Direccion</th>
              <th>Telefono</th>
              <th>Email</th>
              <th>Responsable</th>
              <th>Estado</th>
              <th></th>
          </tr>
      </thead>
      <tbody>

      </tbody>
    </table>
</div>

@section Scripts{
    <script src="~/js/Deposito.js"></script>
}
